<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zc.student_dev.mapper.RoleMapper">

    <select id="queryById" parameterType="long" resultType="java.lang.String">
        select
            `role_code`
        from
            role
        where id=#{id}
    </select>

</mapper>
